{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"allowImportingTsExtensions": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "Preserve",
		"moduleResolution": "bundler",
		"baseUrl": "./",
		"paths": {
			"$lib/*": ["./src/lib/*"]
		},
	},
}
